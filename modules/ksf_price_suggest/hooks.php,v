1.4
log
@Modify for use on new module CALC_PRICING
@
text
@<?php
define ('SS_CALC_PRICING', 111<<8);

class hooks_CALC_PRICING extends hooks {
	var $module_name = 'CALC_PRICING'; 

	/*
		Install additonal menu options provided by module
	*/
	function install_options($app) {
		global $path_to_root;

		switch($app->id) {
			case 'stock':
				$app->add_rapp_function(2, _('CALC_PRICING'), 
					$path_to_root.'/modules/CALC_PRICING/CALC_PRICING.php', 'SA_CALC_PRICING');
		}
	}

	function install_access()
	{
		$security_sections[SS_CALC_PRICING] = _("CALC_PRICING");

		$security_areas['SA_CALC_PRICING'] = array(SS_CALC_PRICING|101, _("CALC_PRICING"));

		return array($security_areas, $security_sections);
	}
}
?>
